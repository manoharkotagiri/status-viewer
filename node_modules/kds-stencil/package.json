    "build": "npm run clean:all && npm run build:rollup && npm run build:with:docs && npm run build:icons:all && npm run build:without:docs && npm run cp:docs:dist",
    "build:accent-icons": "node scripts/generate-icons.js -p 'kds-accent-icon-' -t scripts/generate-icons/templates/kds-accent-icon-template.js -d src/components/kds-accent-icon/icons node_modules/kds-icons/accent-icons",
    "build:icons": "node scripts/generate-icons.js -p 'kds-icon-' -t scripts/generate-icons/templates/kds-icon-template.js -d src/components/kds-icon/icons node_modules/kds-icons/icons",
    "build:icons:all": "npm run build:icons && npm run build:accent-icons",
    "build:rollup": "npx rollup --config rollup.config.js",
    "build:with:docs": "stencil build --docs --docs-json docs/docs.json",
    "build:without:docs": "stencil build",
    "clean:accent-icons": "rm -rf src/components/kds-accent-icon/icons",
    "clean:all": "npm run clean:dist && npm run clean:accent-icons && npm run clean:icons && npm run clean:www && npm run clean:docs",
    "clean:dist": "rm -rf dist && mkdir -p dist",
    "clean:docs": "rm -rf docs",
    "clean:icons": "rm -rf src/components/kds-icon/icons",
    "clean:www": "rm -rf www",
    "cp:docs:dist": "mkdir -p dist/docs && cp docs/docs.json dist/docs; mkdir -p dist/docs/components && cp -a src/docs/. dist/docs/components && find docs/ -name '*.md' -type f -exec cp {} dist/docs/components \\; && find src/css-only-components/ -name '*.md' -type f -exec cp {} dist/docs/components \\;",
    "prettier": "prettier --trailing-comma es5 --write 'src/**/*.?(ts|tsx|js)' 'scripts/*.?(ts|tsx|js)'",
    "start": "npm run clean:all; npm run build:icons:all; npm run build:rollup; stencil build --dev --watch --serve;",
    "start:es5": "npm run clean:all; npm run build:icons:all; npm run build:rollup; stencil build --dev --watch --es5 --serve;",
    "test": "npm run clean:icons && npm run build:icons && stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "test:ci": "npm run build:icons:all; npm run build:rollup; stencil test --spec --e2e --ci",
    "test:wdio:bs": "wdio ./tests/config/wdio.bs.conf.js",
    "test:wdio:local": "wdio ./tests/config/wdio.local.conf.js"
