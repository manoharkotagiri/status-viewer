    "test:visual:comparison:bs:chrome": "wdio ./tests/config/wdio.bs.chrome.conf.js --suite comparison",
    "test:visual:comparison:bs:firefox": "wdio ./tests/config/wdio.bs.firefox.conf.js --suite comparison",
    "test:visual:comparison:bs:ie": "wdio ./tests/config/wdio.bs.ie.conf.js --suite comparison",
    "test:visual:comparison:bs:safari": "wdio ./tests/config/wdio.bs.safari.conf.js --suite comparison",
