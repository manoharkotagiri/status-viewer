  "scripts": {
    "build": "npm run build:prod",
    "build:accent-icons": "svgr --config ./bin/svgr/ --template ./bin/svgr/accent-icon.template.js -d src/components/AccentIcon/icons node_modules/kds-icons/accent-icons",
    "build:accent-icons:ifmissing": "[ -f ./src/components/AccentIcon/index.js ] && echo '✔ Accent Icons already built' || npm run build:accent-icons",
    "build:beta": "npm run clean && npm run build:icons:all && npm run build:styles && npm run build:rollup",
    "build:icons": "svgr --config ./bin/svgr/ --template ./bin/svgr/icon.template.js -d src/components/Icon/icons node_modules/kds-icons/icons",
    "build:icons:all": "npm run build:icons && npm run build:accent-icons && node ./bin/generate-icon-index-and-markdown.js",
    "build:icons:ifmissing": "[ -f ./src/components/Icon/index.js ] && echo '✔ Icons already built' || npm run build:icons",
    "build:prod": "npm run clean && npm run build:icons:all && npm run build:styles && npm run build:rollup:prod",
    "build:rollup": "NODE_OPTIONS=\"--max-old-space-size=4096\" npx rollup --config rollup.config.js",
    "build:rollup:prod": "NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=4096\" npx rollup --config rollup.config.js",
    "build:styleguide": "npx styleguidist build",
    "build:styles": "node ./bin/copy-styles-to-root.js theme=e-commerce",
    "clean": "npm run clean:build && npm run clean:icons:all",
    "clean:accent-icons": "rm -rf src/components/AccentIcon/icons && rm -f src/components/AccentIcon/index.js",
    "clean:build": "rm -rf build",
    "clean:icons": "rm -rf src/components/Icon/icons && rm -f src/components/Icon/index.js",
    "clean:icons:all": "npm run clean:icons && npm run clean:accent-icons",
    "clean:styleguide": "rm -rf pages-build",
    "lint": "npm run lint-tool -- '**/*.{js,jsx}'",
    "lint-tool": "prettier-standard",
    "lint-tool:css": "stylelint",
    "lint:all": "npm run lint && npm run lint:css:fix",
    "lint:css": "npm run lint-tool:css -- 'src/**/*.css'",
    "lint:css:fix": "stylelint --fix 'src/**/*.css'",
    "prestart": "npm run build:icons:ifmissing && npm run build:accent-icons:ifmissing",
    "pretest": "npm run prestart",
    "start": "NODE_ENV=development styleguidist server",
    "test": "NODE_ENV=test jest",
    "test:visual:baseline:bs:chrome": "wdio ./tests/config/wdio.bs.chrome.conf.js --suite baseline",
    "test:visual:baseline:bs:firefox": "wdio ./tests/config/wdio.bs.firefox.conf.js --suite baseline",
    "test:visual:baseline:bs:ie": "wdio ./tests/config/wdio.bs.ie.conf.js --suite baseline",
